{
    "main": "orbit",
    "output": "builds/orbit.phar",
    "chmod": "0755",
    "finder": [
        {
            "in": ["stubs"],
            "notName": ["*.tar.gz"],
            "ignoreVCS": true
        }
    ],
    "directories": [
        "app",
        "app/Commands",
        "bootstrap",
        "config",
        "vendor"
    ],
    "files": [
        "composer.json"
    ],
    "exclude-composer-files": false,
    "compression": "GZ",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\Json"
    ],
    "blacklist": [
        "tests"
    ],
    "check-requirements": false
}
